  "dependencies": {
    "browserify": "^16.5.1",
    "http-server": "^0.12.3",
    "tsify": "^4.0.2",
    "typed-html": "^2.0.0",
    "typescript": "^3.9.5",
    "watchify": "^3.11.1"
  }
}
