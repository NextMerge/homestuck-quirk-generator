{
  "name": "homestuck-quirk-generator",
  "version": "3.0.0",
  "description": "A site that allows you to convert inputted text into the typing quirk of a Homestuck character.",
  "main": "Src/index.ts",
  "scripts": {
    "watch": "watchify Src/index.ts --debug -p [ tsify --noImplicitAny ] -o App/bundle.js",
    "serve": "live-server --open=App/ --watch=App/",
    "deploy-live": "browserify Src/index.ts -p [ tsify --noImplicitAny ] > App/bundle.j"
  },
  "repository": {
    "type": "git",
    "url": "git+https://QuaternionMark@github.com/QuaternionMark/Homestuck-Quirk-Generator.git"
  },
  "author": "Mark Jarjour",
  "license": "SEE LICENSE IN LICENSE.md",
  "bugs": {
    "url": "https://github.com/QuaternionMark/Homestuck-Quirk-Generator/issues"
  },
  "homepage": "https://quaternionmark.github.io/homestuck_quirk/",
  "dependencies": {
    "@types/materialize-css": "^1.0.8",
    "browserify": "^16.5.1",
    "tsify": "^4.0.2",
    "typed-html": "^2.0.0",
    "typescript": "^3.9.5"
  },
  "devDependencies": {
    "live-server": "^1.2.1",
    "watchify": "^3.11.1"
  }
}
